<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="Ekarta_ServisBinding" />
                <binding name="Ekarta_AdminBinding" closeTimeout="00:01:00" openTimeout="00:01:00"
